{
  "name": "handlerbars-gulp-browserify-babrlify",
  "version": "1.0.0",
  "description": "handlerbars jquery gulp browserify babel babrlify es6",
  "main": "src/index.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "config": {
    "bower_directory": "src/vendors"
  },
  "scripts": {
    "bower": "bower",
    "postinstall": "bower install -p",
    "rm": "rm -fr dist",
    "gulp": "gulp",
    "start": "gulp watch",
    "prestart": "npm install"
  },
  "author": "Maksim Kostromin",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "bootstrap": "^3.3.6",
    "bower": "^1.7.7",
    "browserify": "^13.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.3.1",
    "gulp-sourcemaps": "^1.6.0",
    "handlebars": "^4.0.5",
    "jquery": "^2.2.0",
    "require-dir": "^0.3.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/handlerbars-gulp-browserify-babrlify.git"
  },
  "keywords": [
    "handlerbars",
    "jquery",
    "gulp",
    "browserify",
    "babel",
    "babrlify",
    "es6"
  ],
  "bugs": {
    "url": "https://github.com/daggerok/handlerbars-gulp-browserify-babrlify/issues"
  },
  "homepage": "https://github.com/daggerok/handlerbars-gulp-browserify-babrlify#readme"
}
